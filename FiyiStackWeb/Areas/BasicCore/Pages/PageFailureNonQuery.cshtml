        NonQueryText = "Add";
        NonQueryIcon = "fas fa-plus";

        FailureId = 0;
        HTTPCode = 0;
        EmergencyLevel = 0;
        Message = "";
        StackTrace = "";
        Source = "";
        Comment = "";
        UserCreationId = 0;
        UserLastModificationId = 0;
        
    }
    else
    {
        NonQueryText = "Edit";
        NonQueryIcon = "fas fa-pen";

        FailureModel FailureModel = new FailureModel().Select1ByFailureIdToModel
        (Convert.ToInt32(HttpContext.Request.QueryString.Value.Replace("?FailureId=","")));

        FailureId = FailureModel.FailureId;
        HTTPCode = FailureModel.HTTPCode;
        EmergencyLevel = FailureModel.EmergencyLevel;
        Message = FailureModel.Message;
        StackTrace = FailureModel.StackTrace;
        Source = FailureModel.Source;
        Comment = FailureModel.Comment;
        UserCreationId = FailureModel.UserCreationId;
        UserLastModificationId = FailureModel.UserLastModificationId;
        
    }

